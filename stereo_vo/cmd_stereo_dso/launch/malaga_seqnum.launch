<launch>
  <arg name="seqnum" default="10" />
  <arg name="files_path" default="/media/sysu/200GB_Other/Dataset/Slam/malaga/sequence/$(arg seqnum)/muti_robot/" />
  <arg name="bag_path" default="/media/sysu/200GB_Other/Dataset/Slam/malaga/sequence/$(arg seqnum)/muti_robot/" />
  <arg name="left_topic" default="camera/left/image_raw" />
  <arg name="right_topic" default="camera/right/image_raw" />

  <arg name="id" default="2" />
  <node name="stereo_dso" pkg="cmd_stereo_dso" type="dso_dataset_ros" output="screen" required="true">
    <param name="files" value="$(arg files_path)/$(arg id)" />
    <param name="bag" value="$(arg bag_path)/$(arg id)/$(arg id).bag"/>
    <param name="calib" value="$(find cmd_stereo_dso)/calib/malaga/cam.txt" />
    <param name="topic0" value="$(arg left_topic)" />
    <param name="topic1" value="$(arg right_topic)" />
    <param name="preset" value="0" />
    <param name="mode" value="1" />
    <param name="quiet" value="1" />
    <param name="nomt" value="1" />
  </node>
</launch>
